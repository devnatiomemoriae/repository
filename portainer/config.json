{
  "name": "PortainerX",
  "version": "1.2.2",
  "slug": "portainerx",
  "description": "Manage your Docker environment with ease (with Web UI)",
  "url": "https://github.com/devnatiomemoriae/addon-portainer",
  "advanced": false,
  "ingress": false,
  "startup": "services",
  "panel_icon": "mdi:docker",
  "homeassistant": "0.92.0b2",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "init": false,
  "map": [
    "ssl"
  ],
  "ports": {
    "8000/tcp": null,
    "9000/tcp": 9000
  },
  "ports_description": {
    "8000/tcp": "Edge Agent Api (Enable when managing remote edge agents)",
    "9000/tcp": "Web Agent UI"
  },
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "docker_api": true,
  "options": {},
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "agent_secret": "str?"
  },  
  "image": "hassioaddons/portainer-{arch}"
}
